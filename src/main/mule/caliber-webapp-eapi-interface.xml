        </ee:transform>
    </flow>
    <flow name="get:\aggregate\type:caliber-webapp-eapi-config">
        <flow-ref doc:name="aggregated-assessment-type" doc:id="08c6edff-25e0-4d35-92f9-b97c0a8bd0e8" name="aggregated-assessment-type"/>
