    <flow name="get:\report\(trainerEmail):caliber-webapp-eapi-config">
        <set-variable value="#[attributes.uriParams.trainerEmail]" doc:name="trainerEmail" doc:id="be49e666-c13e-4177-8457-61c0c9ec32cc" variableName="trainerEmail" />
