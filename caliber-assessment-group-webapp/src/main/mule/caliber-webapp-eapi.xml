    <flow name="get:\percentage\(period):caliber-webapp-eapi-config">
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core">
            <ee:variables>
                <ee:set-variable variableName="period">attributes.uriParams.'period'</ee:set-variable>
            </ee:variables>
        </ee:transform>
        <ee:transform xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
            <ee:message>
                <ee:set-payload><![CDATA[%dw 2.0
output application/json
---
{
  PoorPer: "25",
  AveragePer: "40",
  GoodPer: "25",
  SuperstarPer: "10"
}]]></ee:set-payload>
            </ee:message>
        </ee:transform>
    </flow>
    <flow name="get:\aggregate\type:caliber-webapp-eapi-config">
		<flow-ref doc:name="Aggregated Type" doc:id="83945ebc-7700-472f-817b-820950398bf1" name="aggregated-assessment-type"/>
    </flow>
    <flow name="get:\report\utilization:caliber-webapp-eapi-config">
